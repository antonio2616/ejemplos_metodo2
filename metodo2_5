
public class metodo2_5 {

	public static void main(String[] args) {
restar(100,5);
sumar(23,43);

		
	}
		public  static void restar( int x, int y) {
			int restar;
			restar = x-y;
			
					System.out.println("El resultado de la resta es:"+restar);
		


}
	public  static void sumar( int x, int y) {
		int sumar;
		sumar =x+y ;
		
				System.out.println("El resultado de la suma es:"+sumar);
	}

}
